{
  "name": "@wikipathways/pvjs",
  "version": "3.6.1-beta",
  "description": "Diagram viewer and editor for biological pathways.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.",
  "typings": "lib/index",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm-run-all build:lib build:dist",
    "build:lib": "npm run rimraf ./lib && tsc --project tsconfig.json && npm run assets",
    "build:dist": "npm run rimraf ./dist && webpack --config webpack.prod.config.js",
    "assets": "npm run gulp copy:assets",
    "test:e2e": "npm run serve && npm run serve:bundle",
    "serve:bundle": "npm run opn ./test/e2e/bundle-test.html",
    "serve": "webpack-dev-server --config webpack.dev.config.js --open --progress --inline",
    "start": "npm run serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "gulp": "gulp",
    "rimraf": "rimraf",
    "opn": "opn"
  },
  "watch": {
    "test": [
      "main.js",
      "test/*.{js,jsx}"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/pvjs.git"
  },
  "keywords": [
    "pvjs",
    "network",
    "pathway",
    "biology"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/pvjs/issues"
  },
  "homepage": "https://wikipathways.github.io/pvjs/docs/",
  "dependencies": {
    "bridgedb": "^6.0.0-14",
    "csstips": "^0.2.0",
    "csv-streamify": "^1.0.0",
    "csx": "^8.2.1",
    "custom-elements": "^1.0.2",
    "document-register-element": "^1.4.1",
    "gpml2pvjson": "2.0.0-beta.10",
    "js-base64": "^2.1.9",
    "lodash": "^4.14.36",
    "react": "^15.6.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.5.4",
    "react-selectize": "^2.1.0",
    "react-spinkit": "^3.0.0",
    "rgbcolor": "^1.0.0",
    "roboto-fontface": "^0.7.0",
    "rx-extra": "^2.0.3",
    "rxjs": "^5.2.0",
    "svg-pan-zoom": "^3.5.1",
    "tower-strcase": "~0.5.0",
    "typestyle": "^0.22.3",
    "url-loader": "^0.5.8",
    "valid-data-url": "^0.1.3",
    "web-animations-js": "^2.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/assert": "0.0.28",
    "@types/bootstrap": "^3.3.33",
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.4",
    "@types/js-base64": "^2.1.5",
    "@types/jsonstream": "^0.8.27",
    "@types/node": "^6.0.73",
    "@types/react": "^15.0.28",
    "@types/react-dom": "^0.14.23",
    "@types/urijs": "^1.15.32",
    "@types/whatwg-fetch": "0.0.33",
    "autoprefixer": "^6.7.7",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bdd-with-opts": "~1.0.0",
    "blessed": "^0.1.81",
    "browser-sync": "^2.13.0",
    "chai": "~1.10.0",
    "chai-as-promised": "~4.1.1",
    "colors": "~1.0.3",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "deep-diff": "^0.3.8",
    "deglobalify": "^0.2.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fetch-mock": "^5.11.0",
    "file-loader": "^0.10.1",
    "freeport": "~1.0.4",
    "global": "^4.3.2",
    "graceful-fs": "^4.1.4",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.4.0",
    "gulp-concat-css": "^2.3.0",
    "gulp-copy": "^1.0.0",
    "html-webpack-plugin": "^2.24.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "inquirer": "~0.8.0",
    "jaguarjs-jsdoc": "0.0.1",
    "jest": "^20.0.4",
    "jsdoc": "^3.4.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-firefox-launcher": "0.1.7",
    "karma-growl-reporter": "0.1.1",
    "karma-mocha": "0.2.2",
    "karma-mocha-reporter": "2.0.0",
    "mkdirp": "~0.5.1",
    "mocha": "^2.5.3",
    "mockserver": "^1.5.1",
    "npm-run-all": "^2.1.1",
    "npm-watch": "^0.1.9",
    "opn-cli": "^3.1.0",
    "postcss-loader": "^1.3.3",
    "pretty-data": "~0.40.0",
    "pretty-hrtime": "~0.2.2",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "rsyncwrapper": "~0.4.1",
    "rx-fs": "github:ariutta/rx-fs#03425b37820b71ab4e8947a6e14fa5ef8135e990",
    "sax": "~0.6.1",
    "selenium-launcher": "~1.1.10",
    "sinon": "~1.12.1",
    "spawn-mocha-parallel": "~1.0.0",
    "style-loader": "^0.16.1",
    "svg-url-loader": "^2.0.2",
    "through": "~2.3.6",
    "ts-jest": "^20.0.5",
    "ts-loader": "^2.1.0",
    "typescript": "2.2.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^1.16.5",
    "webpack-hot-middleware": "^2.18.0",
    "yargs": "~1.3.3"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
}
